<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   >






    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guidelineV005"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05"
         />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guidelineV035"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.35"
       />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guidelineV070"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.65"
        />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guidelineV095"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95"
         />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guidelineH015"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.15"
        />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guidelineH040"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.4"
        />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guidelineH065"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.65"
        />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guidelineH090"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9"
        />

    <ImageView
        android:id="@+id/ivTopLeft"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guidelineH040"
        app:layout_constraintLeft_toLeftOf="@+id/guidelineV005"
        app:layout_constraintRight_toLeftOf="@+id/guidelineV035"
        app:layout_constraintTop_toTopOf="@+id/guidelineH015"
        app:srcCompat="@android:color/white"
        />

    <ImageView
        android:id="@+id/ivTopCenter"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guidelineH040"

        app:layout_constraintLeft_toLeftOf="@+id/guidelineV035"
        app:layout_constraintRight_toLeftOf="@+id/guidelineV070"
        app:layout_constraintTop_toTopOf="@+id/guidelineH015"

        app:srcCompat="@android:color/white" />

    <ImageView
        android:id="@+id/ivTopRight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guidelineH040"

        app:layout_constraintLeft_toLeftOf="@+id/guidelineV070"
        app:layout_constraintRight_toLeftOf="@+id/guidelineV095"
        app:layout_constraintTop_toTopOf="@+id/guidelineH015"

        app:srcCompat="@android:color/white" />

    <ImageView
        android:id="@+id/ivCenterLeft"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guidelineH065"

        app:layout_constraintLeft_toLeftOf="@+id/guidelineV005"
        app:layout_constraintRight_toLeftOf="@+id/guidelineV035"
        app:layout_constraintTop_toTopOf="@+id/guidelineH040"

        app:srcCompat="@android:color/white" />




    <ImageView
        android:id="@+id/ivCenter"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guidelineH065"
        app:layout_constraintLeft_toLeftOf="@+id/guidelineV035"
        app:layout_constraintRight_toLeftOf="@+id/guidelineV070"
        app:srcCompat="@android:color/white"
        app:layout_constraintTop_toTopOf="@+id/guidelineH040" />

    <ImageView
        android:id="@+id/ivCenterRight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guidelineH065"
        app:layout_constraintLeft_toLeftOf="@+id/guidelineV070"
        app:layout_constraintRight_toLeftOf="@+id/guidelineV095"
        app:layout_constraintTop_toTopOf="@+id/guidelineH040"
        app:srcCompat="@android:color/white"
        />

    <ImageView
        android:id="@+id/ivBottomLeft"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guidelineH090"
        app:layout_constraintLeft_toLeftOf="@+id/guidelineV005"
        app:layout_constraintRight_toLeftOf="@+id/guidelineV035"
        app:layout_constraintTop_toTopOf="@+id/guidelineH065"
        app:srcCompat="@android:color/white" />

    <ImageView
        android:id="@+id/ivBottomCenter"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guidelineH090"
        app:layout_constraintLeft_toLeftOf="@+id/guidelineV035"
        app:layout_constraintRight_toLeftOf="@+id/guidelineV070"
        app:layout_constraintTop_toTopOf="@+id/guidelineH065"
        app:srcCompat="@android:color/white" />

    <ImageView
        android:id="@+id/ivBottomRight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guidelineH090"
        app:layout_constraintLeft_toLeftOf="@+id/guidelineV070"
        app:layout_constraintRight_toLeftOf="@+id/guidelineV095"
        app:layout_constraintTop_toTopOf="@+id/guidelineH065"
        app:srcCompat="@android:color/white" />

    <View
        android:id="@+id/line1"
        android:layout_width="4dp"
        android:layout_height="0dp"
        android:background="?attr/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/guidelineH090"
        app:layout_constraintLeft_toRightOf="@+id/ivTopLeft"


        app:layout_constraintRight_toLeftOf="@+id/ivTopCenter"
        app:layout_constraintTop_toTopOf="@+id/guidelineH015" />

    <View
        android:id="@+id/line2"
        android:layout_width="4dp"
        android:layout_height="0dp"
        android:background="?attr/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/guidelineH090"
        app:layout_constraintLeft_toRightOf="@+id/ivTopCenter"


        app:layout_constraintRight_toLeftOf="@+id/ivTopRight"
        app:layout_constraintTop_toTopOf="@+id/guidelineH015" />

    <View
        android:id="@+id/line3"
        android:layout_width="0dp"
        android:layout_height="4dp"
        android:background="?attr/colorPrimary"

        app:layout_constraintBottom_toTopOf="@+id/ivCenterLeft"
        app:layout_constraintLeft_toLeftOf="@+id/guidelineV005"


        app:layout_constraintRight_toLeftOf="@+id/guidelineV095"
        app:layout_constraintTop_toBottomOf="@+id/ivTopLeft" />

    <View
        android:id="@+id/line4"
        android:layout_width="0dp"
        android:layout_height="4dp"
        android:background="?attr/colorPrimary"

        app:layout_constraintBottom_toTopOf="@+id/ivBottomLeft"
        app:layout_constraintLeft_toLeftOf="@+id/guidelineV005"


        app:layout_constraintRight_toLeftOf="@+id/guidelineV095"
        app:layout_constraintTop_toBottomOf="@+id/ivCenterLeft" />

    <ProgressBar
        android:id="@+id/progressBar"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:indeterminate="true"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/guidelineV005"
        app:layout_constraintRight_toLeftOf="@+id/btnClear"
        android:layout_marginRight="0dp"
        app:layout_constraintTop_toTopOf="@+id/guidelineH090"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp" />








</android.support.constraint.ConstraintLayout>
